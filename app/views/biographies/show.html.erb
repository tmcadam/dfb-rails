<h3>
  <strong><%= @biography.title %></strong>
</h3>
<p>
    <h4><%= @biography.lifespan %></h4>
</p>

<%= tag.div @biography.body_with_images.html_safe, class:"biography_body" %>

<% if @biography.authors.present? %>
    <p class="authors">
        <strong>Authors:</strong> <%= @biography.authors %>
    </p>
<% end %>
